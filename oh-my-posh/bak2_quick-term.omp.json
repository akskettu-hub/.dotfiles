{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "#74c7ec",
          "foreground": "#cdd6f4",
          "properties": {
            "alpine": "\uf300",
            "arch": "\uf303",
            "centos": "\uf304",
            "debian": "\uf306",
            "elementary": "\uf309",
            "fedora": "\uf30a",
            "gentoo": "\uf30d",
            "linux": "\ue712",
            "macos": "\ue711",
            "manjaro": "\uf312",
            "mint": "\uf30f",
            "opensuse": "\uf314",
            "raspbian": "\uf315",
            "ubuntu": "\uf31c",
            "windows": "\ue70f"
          },
          "style": "diamond",
          "leading_diamond": "\u256d\u2500\ue0b2",
          "template": " {{ .Icon }} ",
          "type": "os"
        },
        {
          "background": "#f2cdcd",
          "foreground": "#45475a",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " {{ .UserName }} ",
          "type": "session"
        },
        {
          "background": "#94E2D5",
          "foreground": "#45475a",
          "style": "powerline",
          "powerline_symbol": "\ue0b0",
          "template": " {{ if .Venv }}{{ .Venv }}{{ end }} ",
          "type": "python"
        },
        
        {
          "background": "#a6E3a1", 
          "foreground": "#45475a",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#fab387{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#fab387{{ end }}",
            "{{ if gt .Ahead 0 }}#cba6f7{{ end }}",
            "{{ if gt .Behind 0 }}#cba6f7{{ end }}"
          ],
          "properties": {
            "branch_template": "{{ trunc 25 .Branch }}",
            "fetch_stash_count": true,
            "fetch_status": true,
            "branch_icon": "\uf418 ",
            "branch_identical_icon": "\uf444",
            "branch_gone_icon": "\ueab8"
          },
          "style": "diamond",
          "leading_diamond": "<transparent,background>\ue0b0</>",
          "template": " {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} <transparent>\ue0b1</> <#45475a>\uf044 {{ .Working.String }}</>{{ end }}{{ if .Staging.Changed }} <transparent>\ue0b1</> <#45475a>\uf046 {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }} <transparent>\ue0b1</> <#45475a>\ueb4b {{ .StashCount }}</>{{ end }} ",
          "trailing_diamond": "\ue0b0",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#eba0ac",
          "foreground": "#45475a",
          "properties": {
            "style": "austin",
            "always_enabled": true
          },
          "invert_powerline": true,
          "style": "powerline",
          "powerline_symbol": "\ue0b2",
          "template": " \ueba2 {{ .FormattedMs }} ",
          "type": "executiontime"
        },
        {
          "background": "#74c7ec",
          "foreground": "#45475a",
          "properties": {
            "time_format": "15:04:05"
          },
          "invert_powerline": true,
          "style": "diamond",
          "template": " \ue641 {{ .CurrentDate | date .Format }} ",
          "trailing_diamond": "\ue0b0",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#74c7ec",
          "properties": {
            "style": "agnoster_short",
            "max_depth": 3,
            "folder_icon": "\u2026",
            "folder_separator_icon": " > "
          },
          "style": "plain",
          "template": "\u2502  {{ .Path }} ",
          "type": "path"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#74c7ec",
          "style": "plain",
          "template": "\u2570\u2500 {{ if .Root }}#{{else}}${{end}}",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
